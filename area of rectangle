#include <bits/stdc++.h>  //includes all needed header files
using namespace std;  //avoids the need for std:: 

//Area of a rectangle

//declaring a variable means we are in control of memory(we can choose size)

int main()
{
    long l,b,area;      //since we had constraint l,b could be from 1-10^9 , int is only 4 bytes, long - 8 bytes
    cin >> l >> b;    //cin for scanf ; cout for printf
    area=l*b;
    cout << "Area=" << area;
    
    return 0;
}
